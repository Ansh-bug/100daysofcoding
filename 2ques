#include <stdio.h>

int main() {
    int a, b;
    int sum, diff, prod, quot;

    printf("Enter first number: ");
    scanf("%d", &a);

    printf("Enter second number: ");
    scanf("%d", &b);

    sum = a + b;
    diff = a - b;
    prod = a * b;

    if (b != 0)
        quot = a / b;
    else
        quot = 0;

    printf("Sum = %d\n", sum);
    printf("Difference = %d\n", diff);
    printf("Product = %d\n", prod);

    if (b != 0)
        printf("Quotient = %d\n", quot);
    else
        printf("Division by zero not allowed.\n");

    return 0;
}

